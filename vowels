import sys
l=[]
for i in sys.stdin :
	num = i
	break
num1 = int(num)
for i in sys.stdin:
    l.append(i)
print (l)
for i in range(0,num1):
	l[i] = l[i].rstrip()
	if(l[i] =='a') or (l[i]=='e') or (l[i]=='i') or (l[i]=='o') or (l[i]=='u'):
		print ("Vowels")
	else :
		print ("Consonant")# your code goes here
